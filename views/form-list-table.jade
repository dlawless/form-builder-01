table.table.table-striped.table-condensed
  thead
    tr
      th Name
      th Display Name
  tbody
    tr(ng-repeat='form in forms')
      td {{form.name}}
      td {{form.displayName}}
      td
        a(href='#/edit/{{form._id}}') Edit&nbsp;|
        a(href='/formRender/{{form._id}}', target='_blank') &nbsp;Preview&nbsp;|
        a(href='#/responses/{{form._id}}', target='_blank') &nbsp;Responses